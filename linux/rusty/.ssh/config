Host gitlab.com
    IdentityFile ~/.ssh/mbp-rbunch_rsa

Host *-sms*
    User root
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null

Host *-smw*
    User root
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null

Host buildservice buildservice.us.cray.com
    User build
    IdentityFile ~/.ssh/cray_rsa

Host *
    IdentityFile ~/.ssh/cray_rsa

